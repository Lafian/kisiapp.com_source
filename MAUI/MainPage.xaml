<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MAUI"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             xmlns:Shared="clr-namespace:Shared;assembly=Shared"
             xmlns:Components="clr-namespace:Shared.Components;assembly=Shared"
             xmlns:Pages="clr-namespace:MAUI.Components.Pages"
             x:Class="MAUI.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type Pages:Home}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>



</ContentPage>
